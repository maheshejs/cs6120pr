@func: int {
.ssa.entry:
  jmp .ssa.first;
.ssa.first:
  n.2: int = const 5;
  ret n.2;
}
@loop(infinite: bool, print: bool) {
.ssa.entry:
  v.3: int = undef;
  v.4: int = id v.3;
  jmp .ssa.first;
.ssa.first:
.entry:
.loop.header:
  br infinite .loop.body .loop.end;
.loop.body:
  v.6: int = id v.4;
  br print .loop.print .loop.next;
.loop.print:
  v.5: int = call @func;
  print v.5;
  v.6: int = id v.5;
.loop.next:
  v.4: int = id v.6;
  jmp .loop.header;
.loop.end:
}
@main {
.ssa.entry:
  jmp .ssa.first;
.ssa.first:
  infinite.9: bool = const false;
  print.10: bool = const true;
  call @loop infinite.9 print.10;
}
